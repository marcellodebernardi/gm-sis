<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<TabPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
         minWidth="-Infinity"
         prefHeight="514.0" prefWidth="701.0" tabClosingPolicy="UNAVAILABLE"
         xmlns="http://javafx.com/javafx/8.0.111" fx:controller="controllers.common.MenuController">
    <tabs>
        <Tab text="Untitled Tab 1">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
            </content>
        </Tab>
        <Tab text="Specialist Repair Center">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="487.0" prefWidth="687.0">
                    <children>
                        <TextField fx:id="field_searchSRC" layoutX="5.0" layoutY="65.0" onAction="#searchSRC"
                                   prefHeight="27.0" prefWidth="542.0"
                                   promptText="Search Specialist Repair Centers..."/>
                        <Button fx:id="btn_search" layoutX="548.0" layoutY="65.0" mnemonicParsing="false"
                                onAction="#searchSRC" prefHeight="27.0" prefWidth="148.0" text="Search"/>
                        <Button fx:id="btn_addSpecialistBooking" layoutX="5.0" layoutY="14.0" mnemonicParsing="false"
                                onAction="#addSRCBooking" prefHeight="27.0" prefWidth="145.0" text="Create booking"/>
                        <Button fx:id="btn_deleteSpecialistBooking" layoutX="159.0" layoutY="14.0"
                                mnemonicParsing="false" onAction="#deleteSRCBooking" prefHeight="27.0" prefWidth="145.0"
                                text="Delete Booking"/>
                        <Button fx:id="btn_editSpecialistBooking" layoutX="314.0" layoutY="14.0" mnemonicParsing="false"
                                onAction="#editSRCBooking" prefHeight="27.0" prefWidth="132.0" text="Modify Booking"/>
                        <TableView layoutX="5.0" layoutY="94.0" prefHeight="342.0" prefWidth="691.0">
                            <columns>
                                <TableColumn prefWidth="90.0" text="spc ID:"/>
                                <TableColumn prefWidth="81.0" text="spc Name:"/>
                                <TableColumn prefWidth="123.0" text="spc Address:"/>
                                <TableColumn prefWidth="108.0" text="spc Postcode:"/>
                                <TableColumn prefWidth="138.0" text="spc Phone:"/>
                                <TableColumn prefWidth="151.0" text="spc Email:"/>
                                <TableColumn prefWidth="296.0" text="spc Phone:"/>
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
